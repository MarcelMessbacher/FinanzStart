generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id                         String   @id @default(cuid())
  playerName                 String
  retirementAgeMonths       Int
  passiveIncomeAtRetirement Int
  createdAt                 DateTime @default(now())
}

